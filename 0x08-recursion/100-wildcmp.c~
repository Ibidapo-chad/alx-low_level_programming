#include "holberton.h"

int wildcmp(char *s1, char *s2)
{
  int err = 0;
  if (_strlen_recursion(s1) - _strlen_recursions(str2)>1 || _strlen_recursion(s1) - _strlen_recursion(str2)<1)
    return 0;

  if (*str1 =='\0' && *str2=='\0'|| err==2)
    return err;

  if (*str1!=*str2)
    err++;

  return strcmp1(str1 + 1, str2 + 1);
}
